{
  "name": "pidea-spark",
  "version": "1.0.0",
  "description": "Simple CLI tool for Cursor IDE automation using Chrome DevTools Protocol",
  "main": "pidea-spark.js",
  "type": "module",
  "scripts": {
    "start": "node pidea-spark.js",
    "dev": "node pidea-spark.js --verbose",
    "test": "node pidea-spark.js --dry-run",
    "list": "node pidea-spark.js --list",
    "status": "node pidea-spark.js --status"
  },
  "keywords": [
    "cursor",
    "automation",
    "cdp",
    "playwright",
    "cli",
    "ide-automation"
  ],
  "author": "Patrick B.",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.54.2"
  },
  "path": {
    "base": "./",
    "pidea-spark-output": "./pidea-spark-output",
    "templates": "./automation/templates",
    "games-templates": "./automation/templates/games",
    "webapps-templates": "./automation/templates/webapps",
    "automation": "./automation",
    "workflows": "./automation/workflows",
    "core": "./automation/core",
    "managers": "./automation/managers",
    "ai": "./automation/ai",
    "ui": "./automation/ui",
    "utils": "./automation/utils"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fr4iser90/SimpleCursorAutomation.git"
  },
  "bugs": {
    "url": "https://github.com/fr4iser90/SimpleCursorAutomation/issues"
  },
  "homepage": "https://github.com/fr4iser90/SimpleCursorAutomation#readme"
}
